{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "heartsync": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "less",
          "displayBlock": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "hs",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/heartsync",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "less",
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles/index.less",
              "src/styles/mat.scss",
              "src/styles/uno.css",
              {
                "input": "node_modules/highlight.js/styles/atom-one-dark.css",
                "bundleName": "atom-one-dark",
                "inject": false
              },
              {
                "input": "node_modules/highlight.js/styles/atom-one-light.css",
                "bundleName": "atom-one-light",
                "inject": false
              },
              {
                "input": "src/styles/light.less",
                "bundleName": "light-theme",
                "inject": false
              },
              {
                "input": "src/styles/dark.less",
                "bundleName": "dark-theme",
                "inject": false
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["src/styles", "src/styles/themes"]
            },
            "scripts": [
              {
                "input": "./node_modules/jquery/dist/jquery.min.js",
                "bundleName": "jquery.min",
                "inject": false
              },
              {
                "input": "./node_modules/jquery-ui/dist/jquery-ui.min.js",
                "bundleName": "jquery-ui.min",
                "inject": false
              },
              {
                "input": "./node_modules/ui-contextmenu/jquery.ui-contextmenu.min.js",
                "bundleName": "jquery.ui-contextmenu.min",
                "inject": false
              },
              {
                "input": "./node_modules/jquery.fancytree/dist/jquery.fancytree-all.min.js",
                "bundleName": "jquery.fancytree-all.min",
                "inject": false
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "2MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kB",
                  "maximumError": "1MB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "proxyConfig": "proxy.conf.js",
            "disableHostCheck": true,
            "host": "0.0.0.0",
            "port": 4200
          },
          "configurations": {
            "production": {
              "buildTarget": "heartsync:build:production"
            },
            "development": {
              "buildTarget": "heartsync:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "less",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "src/styles.less"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
